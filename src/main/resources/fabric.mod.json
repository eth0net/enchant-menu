{
  "schemaVersion": 1,
  "id": "enchant-menu",
  "version": "${version}",
  "name": "Enchant Menu",
  "description": "Enchant Menu adds a simple menu for modifying item enchantments.",
  "authors": [
    "eth0net"
  ],
  "contact": {
    "homepage": "https://github.com/eth0net/enchant-menu/",
    "issues": "https://github.com/eth0net/enchant-menu/issues/",
    "sources": "https://github.com/eth0net/enchant-menu/"
  },
  "license": "MIT",
  "icon": "assets/enchant-menu/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.github.eth0net.enchantmenu.EnchantMenu"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.github.eth0net.enchantmenu.client.EnchantMenuClient"
      }
    ]
  },
  "mixins": [],
  "depends": {
    "fabric": "*",
    "fabric-language-kotlin": ">=1.8.2+kotlin.1.7.10",
    "fabricloader": ">=0.14.9",
    "java": ">=17",
    "minecraft": "1.18.2"
  },
  "recommends": {
    "cloth-config": "*",
    "completeconfig-base": "^1.4.1",
    "modmenu": "*"
  }
}
